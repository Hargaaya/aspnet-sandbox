@model List<UserEvent>

<div class="events-booked">
    <h1>My Events</h1>

    <table>
        <tr>
            <th>Title</th>
            <th>Date</th>
            <th>Location</th>
            <th>Organizer</th>
        </tr>

        @foreach (UserEvent ev in Model)
        {
            <tr>
                <td>@ev.Title</td>
                <td>@ev.Date.ToString("d")</td>
                <td>@ev.Place</td>
                <td>@ev.OrganizerId</td>
            </tr>
        }

    </table>
</div>


